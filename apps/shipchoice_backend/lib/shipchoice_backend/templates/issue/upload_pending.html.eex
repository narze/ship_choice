<div class="row">
  <div class="col-lg-12">
    <h4>Upload Kerry Pending Report</h4>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <%= form_for @conn, issue_path(@conn, :upload_pending), [multipart: true], fn f -> %>
      <div class="form-group">
        <label for="file">Kerry Pending Report File</label>
        <%= file_input f, :kerry_pending_report, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= submit "Submit", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
